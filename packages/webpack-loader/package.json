{
  "name": "webpack-loader-rics",
  "version": "0.3.3",
  "description": "Webpack loader for rics CSS preprocessor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --no-dts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "rics": "workspace:*"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0",
    "webpack": "^5.0.0"
  },
  "keywords": [
    "webpack",
    "loader",
    "rics",
    "scss",
    "css",
    "preprocessor"
  ],
  "license": "MIT"
}
