{
  "name": "codemirror-lang-rics",
  "version": "0.3.13",
  "description": "CodeMirror 6 language support for rics",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "rics": "workspace:*"
  },
  "peerDependencies": {
    "@codemirror/language": "^6.0.0",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0"
  },
  "devDependencies": {
    "@codemirror/language": "^6.0.0",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "codemirror",
    "codemirror-6",
    "rics",
    "scss",
    "css",
    "syntax-highlighting",
    "editor"
  ],
  "author": "Boidushya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-lyrics/rics.git",
    "directory": "packages/codemirror"
  },
  "homepage": "https://rics.boidu.dev",
  "bugs": {
    "url": "https://github.com/better-lyrics/rics/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
