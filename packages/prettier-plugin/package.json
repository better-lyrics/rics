{
  "name": "prettier-plugin-rics",
  "version": "0.3.17",
  "description": "Prettier plugin for rics CSS preprocessor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "prettier",
    "prettier-plugin",
    "rics",
    "scss",
    "css",
    "formatter"
  ],
  "author": "Boidushya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-lyrics/rics.git",
    "directory": "packages/prettier-plugin"
  },
  "homepage": "https://rics.boidu.dev",
  "bugs": {
    "url": "https://github.com/better-lyrics/rics/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
