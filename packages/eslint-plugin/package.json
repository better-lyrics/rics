{
  "name": "eslint-plugin-rics",
  "version": "0.3.20",
  "description": "ESLint plugin for rics CSS preprocessor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "rics": "workspace:*"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "rics",
    "scss",
    "css",
    "linter"
  ],
  "author": "Boidushya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-lyrics/rics.git",
    "directory": "packages/eslint-plugin"
  },
  "homepage": "https://rics.boidu.dev",
  "bugs": {
    "url": "https://github.com/better-lyrics/rics/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
